{
  "name": "appium-flutter-driver",
  "description": "Appium Flutter driver",
  "keywords": [
    "appium",
    "flutter"
  ],
  "version": "2.3.0",
  "author": "TruongSinh Tran-Nguyen <i@truongsinh.pro>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appium-userland/appium-flutter-driver.git"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "main": "./build/lib/driver.js",
  "types": "./build/lib/index.d.ts",
  "appium": {
    "driverName": "flutter",
    "automationName": "Flutter",
    "platformNames": [
      "iOS",
      "Android"
    ],
    "mainClass": "FlutterDriver"
  },
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "build",
    "!build/tsconfig.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "npm run build -- --watch",
    "clean": "npm run build -- --clean",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "cp ../README.md ../LICENSE ./",
    "prepare": "npm run clean && npm run build",
    "test": "echo no test",
    "clean-dependency": "rm -rf node_modules && rm -f package-lock.json"
  },
  "peerDependencies": {
    "appium": "^2.0.0"
  },
  "devDependencies": {
    "@appium/eslint-config-appium": "^8.0.4",
    "@appium/eslint-config-appium-ts": "^0.x",
    "@appium/tsconfig": "^0.x",
    "@appium/types": "^0.x",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "semantic-release": "^22.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~5.2"
  },
  "dependencies": {
    "appium-adb": "^11.0.1",
    "appium-android-driver": "^7.0.2",
    "appium-ios-device": "^2.4.1",
    "appium-uiautomator2-driver": "^2.29.4",
    "appium-xcuitest-driver": "^5.8.1",
    "asyncbox": "^3.0.0",
    "bluebird": "^3.1.1",
    "lodash": "^4.0.0",
    "portscanner": "^2.2.0",
    "rpc-websockets": "^7.4.16"
  }
}
